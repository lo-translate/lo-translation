msgstr ""

#  LobbyScript[LobbyScript_3P_BlackLilith_NS2].LobbyScript1[2]
#: LobbyScript[LobbyScript_3P_BlackLilith_NS2].LobbyScript1[2]
msgid " うふ、惚れちゃいました？分かります。"
msgstr ""

#  LobbyScript[LobbyScript_3P_BlackLilith_NS2].LobbyScript1[3]
#: LobbyScript[LobbyScript_3P_BlackLilith_NS2].LobbyScript1[3]
